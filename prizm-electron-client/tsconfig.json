{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"strict": true,
		"jsx": "react-jsx",
		"isolatedModules": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"allowImportingTsExtensions": true,
		"resolveJsonModule": true,
		"noEmit": true,
		"types": ["vite/client"]
	},
	"include": ["src/**/*.ts", "src/**/*.tsx", "src/**/*.d.ts"]
}
